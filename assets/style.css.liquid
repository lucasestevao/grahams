@font-face {
	font-family: 'HelveticaNeueCondensed';
	src: url('mm_helveticaneue-condensed.eot');
	src: url('mm_helveticaneue-condensed.eot') format('embedded-opentype'),
	url('mm_helveticaneue-condensed.woff') format('woff'),
	url('mm_helveticaneue-condensed.ttf') format('truetype'),
	url('mm_helveticaneue-condensed.svg#HelveticaNeueCondensed') format('svg');
}

@font-face {
	font-family: 'HelveticaNeueLightCond';
	src: url('mm_helveticaneue-lightcond.eot');
	src: url('mm_helveticaneue-lightcond.eot') format('embedded-opentype'),
	url('mm_helveticaneue-lightcond.woff') format('woff'),
	url('mm_helveticaneue-lightcond.ttf') format('truetype'),
	url('mm_helveticaneue-lightcond.svg#HelveticaNeueLightCond') format('svg');
}

body {
	font:{{ settings.font_standard_style }} {{ settings.font_standard_size }}px {{ font_standard_type }}, sans-serif;
	line-height: 1.4em;
	color:{{ settings.color_body_standard }};
	background:{{ settings.color_bg_header }};
}

.bodywrap {
	background:{{ settings.color_bg_main }}
	{% unless settings.bg_image_body == '' %}
	{% if settings.bg_offset_body_y == '' %}
	{% assign bg_offset_body_y = '0px' %}
	{% else %}
	{% capture bg_offset_body_y %}{{ settings.bg_offset_body_y }}px{% endcapture %}
	{% endif %}
	url('{{ settings.bg_image_body | asset_url }}') {{ settings.bg_repeat_body | replace: 'top', bg_offset_body_y }}
	{% endunless %};
	position: relative;
	z-index: 5;
	{% if settings.head_foot_border == true %}
		{% if settings.mm_ecommerce_en %}
			border-top: #68C35d 2px solid;
		{% endif %}
	{% else %}
	border-top: none;
	{% endif %}
}

body > .bodywrap {
	position:relative;
	z-index:50;
}

h1, h2, h3, h4, h5, h6 {
	font:{{ settings.font_heading_style }} {{ settings.font_heading_size }}px/{{ settings.font_heading_size }}px {{ font_heading_type }}, sans-serif;
	color:{{ settings.color_body_heading }};
}

{% assign heading_inc = settings.font_heading_size | minus: settings.font_standard_size | divided_by: 6 %}
{% for i in (2..6) %}
h{{i}} {
	font-size: {{ 7 | minus: i | times: heading_inc | plus: settings.font_standard_size }}px;
}
{% endfor %}

h1 a, h2 a, h3 a, h4 a, h5 a, h6 a {
	color: inherit;
	border: none;
}

i, em {
	font-style:italic;
}

b, strong {
	font-weight:bold;
}

a {
	color: {{ settings.color_body_link }};
	text-decoration: none;
}

a.highlight {
	display:inline-block;
	zoom:1;
	*display:inline;
	max-width:100%;
	-moz-transition: box-shadow 200ms; /* Firefox 4 */
	-webkit-transition: box-shadow 200ms; /* Safari and Chrome */
	-o-transition: box-shadow 200ms; /* Opera */
	transition: box-shadow 200ms;
}

a.highlight img {
	z-index:-1;
	position:relative;
}

a.mm_buttom {
	background: #68c35d;
	color: #ffffff;
	font-weight: bold;
	padding: 10px;
	border-color: #68c35d;
	border-bottom: 3px #529a49 solid;
	font-size: 14px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	border-radius: 5px;
	display: block;
	text-align: center;
	margin: 12px 0;
	text-transform: uppercase;
}

#pagefooter #mm_submit:hover,
.content-block .mm_submit:hover,
a.mm_buttom:hover {
	opacity: 90%;
	/* IE 8 */
	-ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=90)";
	/* IE 5-7 */
	filter: alpha(opacity=90);
	/* Netscape */
	-moz-opacity: 0.9;
	/* Safari 1.x */
	-khtml-opacity: 0.9;
	/* Good browsers */
	opacity: 0.9;
}

.error {
	color:{{ settings.color_body_link }};
	font-style:italic;
}

input {
	padding:0;
	height:36px;
	text-indent:6px;
}

.id-contact_store {
	padding: 2px 0 0 0;
	height: 28px;
}

input,
.textboxcont,
textarea,
.id-contact_store {
	background:{{ settings.color_form_field_bg }};
	border:1px solid {{ settings.color_form_field_border }};
	color:{{ settings.color_form_field_text }};
	-moz-transition: border-color 200ms; /* Firefox 4 */
	-webkit-transition: border-color 200ms; /* Safari and Chrome */
	-o-transition: border-color 200ms; /* Opera */
	transition: border-color 200ms;
	-webkit-appearance: none; /* iOS */
	
	-moz-border-radius: 5px 5px 5px 5px;
	-webkit-border-radius: 5px 5px 5px 5px;
	border-radius: 5px 5px 5px 5px;
}

input,
.textboxcont,
textarea {
	font-size:80%;
}

input[type=radio] {
	-webkit-appearance: radio;
}

.textboxcont input {
	border:0;
	height:36px;
	width:100%;
	text-indent:18px;
}

textarea {
	width:98%;
	padding:5px;
	height:100px;
	margin-top:8px;
}

input[type=submit] {
	height: auto;
	text-indent: 0;
	cursor: pointer;
}

input[type=checkbox] {
	vertical-align:middle;
	-webkit-appearance: checkbox;
}

input.short {
	height:30px;
	line-height:30px;
}

.hidden {
	display:none;
}

a.button,
input.button,
.button a {
	border:none !important;
	background:{{ settings.color_btn_bg }};
	font:1.05em {{ font_heading_type }},Arial,sans-serif;
	color: {{ settings.color_btn_text }};
	padding:7px 15px;
	cursor:pointer;
	display:inline-block;
	zoom:1;
	*display:inline;
	
	-webkit-box-shadow: 0 0 2px 0 #000;
	-moz-box-shadow: 0 0 2px 0 #000;
	box-shadow: 0 0 2px 0 #000;

	-webkit-border-radius: 2px;
	-moz-border-radius: 2px;
	border-radius: 2px;
}

a.button.altcolor,
input.button.altcolor,
.button.altcolor a {
	background-color:{{ settings.color_btn_alt_bg }};
}


a.button.large,
input.button.large,
.button.large a {
	padding:15px;
	display:block;
	text-transform:uppercase;
	font-size:20px;
}

a.button.large.plus,
input.button.large.plus,
.button.large.plus a {
	text-align:left;
}

a.button:hover,
input.button:hover,
.button a:hover {
	color: {{ settings.color_btn_text }};
	-webkit-box-shadow: 0 0 2px 1px #000;
	-moz-box-shadow: 0 0 2px 1px #000;
	box-shadow: 0 0 2px 1px #000;
}

.betweenbuttons {
	margin-left:10px;
	margin-right:10px;
}

.loadingimg {
	background:url({{ settings.loading_graphic | asset_url }}) center center no-repeat;
	min-height:40px;
}

