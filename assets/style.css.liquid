{% comment %} Calculate the sprite offset, and create the high-DPI offset too {% endcomment %}
{% if settings.social_sprite_mode == 'light' %}
{% assign social_sprite_offset = -28 %}
{% elsif settings.social_sprite_mode == 'colorful' %}
{% assign social_sprite_offset = -56 %}
{% else %}
{% assign social_sprite_offset = 0 %}
{% endif %}

