#util-bar .socialicons li.twitter a { background-position:{{ social_sprite_offset }}px -56px; }
#util-bar .socialicons li.tumblr a { background-position:{{ social_sprite_offset }}px -84px; }
#util-bar .socialicons li.pinterest a { background-position:{{ social_sprite_offset }}px -112px; }
#util-bar .socialicons li.youtube a { background-position:{{ social_sprite_offset }}px -140px; }
