
/*
    @autor: Marlúcio
    @date: 21/02/2014
    @description: seleciona/retira seleção de todos os objetos.
*/
$(function () {
    $('#selectall').toggle(
        function() {
            $('#storelist li input.mm_check').prop('checked', true);
        },
        function() {
            $('#storelist li input.mm_check').prop('checked', false);
        }
    );
  
});

/*
    @autor: Marlúcio
    @date: 21/02/2014
    @description: força a digitação de apenas números ou traços nos 
    campos que possuem a classe .tel.
*/  
$(function () {  
  $('.tel').numeric('-'); 
});
     
  
